"*":
  core:
    telemetryConsent: "no"
    themes: [
      "one-dark-ui"
      "atom-dark-syntax"
    ]
  editor:
    fontSize: 16
    softWrap: true
    tabLength: 4
    tabType: "soft"
  "markdown-preview":
    liveUpdate: false
    openPreviewInSplitPane: false
    useGitHubStyle: true
  welcome:
    showOnStartup: false
